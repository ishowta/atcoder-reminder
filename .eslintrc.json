{
    "extends": "standard",
    "rules": {
        "no-undef": "warn",
        "no-unused-vars": "warn",
        "camelcase": "off",
        "max-len": [
            "error",
            {
                "code": 120
            }
        ]
    },
    "globals": {
        "window": true,
        "createjs": true,
        "$": true
    }
}
